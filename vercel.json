{
  "redirects": [
    { "source": "/.well-known/tdmrep.json", "destination": "https://nineplus.sh/.well-known/tdmrep.json" },
    { "source": "/robots.txt", "destination": "https://www.nineplus.sh/robots.txt" },
    { "source": "/[.+]", "destination": "/" }
  ],
  "headers": [{
    "source": "/(.*)",
    "headers": [{
      "key": "tdm-policy",
      "value": "https://www.nineplus.sh/legal/tdm.json"
    }, {
      "key": "tdm-reservation",
      "value": "1"
    }, {
      "key": "x-robots-tag",
      "value": "noai"
    }]
  }]
}
